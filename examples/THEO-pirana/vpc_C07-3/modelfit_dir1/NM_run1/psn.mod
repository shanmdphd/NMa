$PROBLEM    THEOPHYLLINE ORAL P:ROOT F:BASE
$INPUT      ID AMT TIME DV BWT
$DATA      THEOPP
$SUBROUTINE ADVAN2 TRANS2
$PK
  TVCL  = THETA(1)
  TVV   = THETA(2)
  TVKA  = THETA(3)
  CL    = TVCL * EXP(ETA(1))
  V     = TVV  * EXP(ETA(2))
  KA    = TVKA * EXP(ETA(3))
  S2    = V
  K     = CL / V

$ERROR
  IPRED  = F
  W     = SQRT(THETA(4)**2 + THETA(5)**2 * IPRED**2)
  IRES  = DV - IPRED
  IWRES  = IRES / W
  Y     = IPRED + W * EPS(1)

$THETA  (0,0.1)
 (0,1)
 (0,10)
 (0,1)
 (0,0.5)
$OMEGA  0.2
 0.2
 0.2
$SIGMA  1  FIX
$ESTIMATION METHOD=COND INTERACTION NOABORT PRINT=5 MAXEVALS=0
$TABLE      ID MDV TIME DV ONEHEADER NOPRINT NOAPPEND FILE=npctab.dta

